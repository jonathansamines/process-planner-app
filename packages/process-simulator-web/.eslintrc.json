{
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "functions": "never"
    }]
  }
}
